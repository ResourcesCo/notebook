    "@unocss/preset-typography": "^0.46.5",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/server-renderer": "^3.2.45",
    "@vue/test-utils": "^2.2.4",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "happy-dom": "^8.1.0",
    "rollup": "^3.9.1",
