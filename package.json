{
  "name": "@resources/notebook",
  "type": "module",
  "scripts": {
    "api": "cross-env PORT=3334 node api/server.js",
    "build:edit": "NODE_ENV=production vite build -c vite-edit.config.ts",
    "build:view": "NODE_ENV=production vite build -c vite-view.config.ts",
    "build:main": "NODE_ENV=production vite build",
    "build:vite": "pnpm build:edit && pnpm build:view && pnpm build:main",
    "build:copy": "mkdir -p dist/app/edit && cp dist-edit/app/edit/index.html dist/app/edit/index.html && mkdir -p dist/app/view && cp dist-view/app/view/index.html dist/app/view/index.html",
    "build": "vue-tsc --noEmit && pnpm build:vite && pnpm build:copy && cp -r vercel.json dist/vercel.json && cp -r api dist/api",
    "dev": "vite --port 3333",
    "test": "vitest --outputTruncateLength=500",
    "test:ui": "vitest --ui --outputTruncateLength=500",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.3.4",
